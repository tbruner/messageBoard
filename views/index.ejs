<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <link href="styles.css" rel="stylesheet" type="text/css" />
  </head>
  <body>
    <header>
      <div><%= title %></div>
    </header>
    <section id="messages">
      <% messages.forEach((message) => { %> <%- include('message', { text:
      message.text, user: message.user }) %> <% }) %>
    </section>
  </body>
</html>
